{
  "name": "auditguardx",
  "version": "1.0.0",
  "private": true,
  "description": "LiquidMetal AI app",
  "scripts": {
    "build": "shx rm -rf dist && tsc -b",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "lint": "eslint . --max-warnings=0",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:vectors": "npm run build && raindrop build deploy && bash ../scripts/test-vector-migration.sh",
    "create-migration": "npx ts-node ./scripts/create_migration.mts",
    "clean-seed": "find ./db -name '*seed*' -type f -delete",
    "seed:createSql": "npx ts-node ./scripts/seed-sql.mts",
    "render-db": "pnpm run clean-seed && pnpm run create-migration && pnpm run seed:createSql"
  },
  "devDependencies": {
    "@liquidmetal-ai/in-memory-d1": "^1.0.7",
    "@liquidmetal-ai/raindrop": "^0.11.0",
    "@types/bcryptjs": "^3.0.0",
    "@types/better-sqlite3": "^7.6.13",
    "better-sqlite3": "^11.9.1",
    "prisma": "^6.5.0",
    "prisma-kysely": "^1.8.0",
    "shx": "^0.4.0",
    "ts-node": "^10",
    "typescript": "^5.0.4",
    "typescript-eslint": "^8.7.0",
    "vitest": "^3.1.3",
    "zx": "^8.5.2"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.922.0",
    "@aws-sdk/s3-request-presigner": "^3.922.0",
    "@huggingface/transformers": "^3.7.6",
    "@liquidmetal-ai/raindrop-framework": "^0.11.0",
    "@modelcontextprotocol/sdk": "^1",
    "@workos-inc/node": "^7.73.0",
    "bcryptjs": "^3.0.2",
    "kysely": "^0.27.2",
    "kysely-d1": "^0.3.0",
    "mammoth": "^1.11.0",
    "pdf-parse": "^2.4.5",
    "pdfjs-dist": "^3.11.174",
    "stripe": "^19.3.1",
    "unpdf": "^1.4.0",
    "zod": "^3"
  }
}
